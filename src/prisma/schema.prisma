generator client {
  provider = "prisma-client"
  output   = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    Int     @id @default(autoincrement())
  username String  @unique
  password String
  roleId Int
  role   Role    @relation(fields: [roleId], references: [id])
}
 
model Role {
  id        Int     @id @default(autoincrement())
  roles     String  @default("USER")
  users    User[]
}



